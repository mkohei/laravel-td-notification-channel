{
    "name": "mkohei/laravel-td-notification-channel",
    "description": "Treasure Data Notification Channel for Laravel",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mkohei\\LaravelTdNotificationChannel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mkohei\\LaravelTdNotificationChannel\\Test\\": "tests/"
        }
      },
    "authors": [
        {
            "name": "Kohei Matsumoto",
            "email": "baberugaberiimeronnv5@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
